{
  "extends": ["eslint: recommended", "prettier", "prettier/react"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "React": "writable"
  },
  "env": {
    "es6": true,
    "browser": true
  }
}